@model IEnumerable<FamilyAuto.Models.Articles>

@{
    ViewBag.Title = "Services";
}

<h2>Services on offer</h2>

<div class="row">
    <div class="col-md-8">
        @foreach (var n in Model)
        {
            <div class="col-md-8">
                @*<p>@Html.DisplayFor(item => n.DateUploaded)</p>*@
                <h4>@Html.DisplayFor(item => n.ArticleTitle)</h4>
                <p>@Html.DisplayFor(item => n.ArticleDescription)</p>
                <hr />
                <br />
            </div>
        }
    </div>
</div>